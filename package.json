{
  "name": "b2c-plugin-data-migrations",
  "version": "0.1.0",
  "description": "B2C CLI plugin for data migrations and feature management",
  "author": "Charles Lavery",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "oclif": {
    "commands": {
      "strategy": "pattern",
      "target": "./dist/commands"
    },
    "topics": {
      "migrations": {
        "description": "Data migrations and feature management"
      },
      "migrations:feature": {
        "description": "Feature deployment and management"
      }
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@oclif/core": "^4",
    "@salesforce/b2c-tooling-sdk": "*"
  },
  "devDependencies": {
    "@oclif/core": "^4",
    "@salesforce/b2c-tooling-sdk": "file:../b2c-cli/packages/b2c-tooling-sdk",
    "@types/node": "^22",
    "typescript": "^5"
  },
  "dependencies": {
    "jszip": "^3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
